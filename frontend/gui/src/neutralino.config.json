{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "js.neutralino.sample",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/target/",
  "url": "/gui-target/decode.html",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": false
  },
  "nativeAllowList": ["app.*", "os.*", "debug.log"],
  "globalVariables": {},
  "modes": {
    "window": {
      "title": "mcd-demo-app",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/icons/appIcon.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    },
    "browser": {
      "globalVariables": {},
      "nativeBlockList": []
    },
    "cloud": {
      "url": "/resources/#cloud",
      "nativeAllowList": ["app.*"]
    },
    "chrome": {
      "width": 800,
      "height": 500,
      "args": "--user-agent=\"Neutralinojs chrome mode\"",
      "nativeBlockList": ["filesystem.*", "os.*"]
    }
  },
  "cli": {
    "binaryName": "mcd-demo-app",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/main.js",
    "binaryVersion": "4.13.0",
    "clientVersion": "3.11.0"
  }
}
